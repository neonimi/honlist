<table class="container-sm">
  <tr>
    <th>ISBNコード</th><th>書名</th><th>価格</th>
    <th>出版社</th><th>刊行日<th><th>ダウンロード</th>
  <tr>
<% @books.each do |book| %>
  <tr>
    <td class="w-25 p-3 scr"><%= book.isbn %></td>
    <td><%= book.title %></td>
    <td><%= book.price %></td>
    <td><%= book.publish %></td>
    <td><%= book.publish %></td>
    <td><%= book.dl %></td>
  </tr>
<% end %>
</table>